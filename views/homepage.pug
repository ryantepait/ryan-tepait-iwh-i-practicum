doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
    div.wrapper
        h1 Games | Custom Object Table
        a(href="/update-cobj") Add New Game
        table 
            thead 
                tr 
                    th Name 
                    th Description 
                    th Price 
            tbody 
                each game in data
                    tr
                        td= game.properties.name
                        td= game.properties.description
                        td= game.properties.price